{
    "name": "Keyframes",
    "short_name": "Keyframes",
    "version": "0.0.81",
    "manifest_version": 2,
    "description": "In-browser CSS Animation creator.",
    "homepage_url": "http://keyframes.app",
    "icons": {
        "16": "icons/keyframes-16.png",
        "48": "icons/keyframes-48.png",
        "128": "icons/keyframes-128.png"
    },
    "browser_action": {
        "default_icon": "icons/keyframes-48.png",
        "default_title": "Keyframes"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "web_accessible_resources": [
        "ui/*.html",
        "styles/css/*.css"
    ],
    "background": {
        "scripts": ["js/background.js"]
    }
}
