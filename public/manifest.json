{
  "manifest_version": 2,
  "name": "React Chrome Extension",
  "version": "0.1.0",
  "icons": {
    "16": "keyframes-16.png",
    "48": "keyframes-48.png",
    "128": "keyframes-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "keyframes-16.png",
      "48": "keyframes-48.png",
      "128": "keyframes-128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "index.html",
    "inject.js",
    "static/*"
  ]
}
